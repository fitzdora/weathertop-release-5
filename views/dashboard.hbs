{{> menu id="dashboard"}}

<section class="ui segment">
  <h2 class="ui header">
    {{o}}
  </h2>
<table class="ui table">
  <thead>
    <th>Code</th>
    <th>Temp</th>
    <th>Wind Speed</th>
  </thead>
  <tbody>
    {{#each station.readings}}
    <tr>
      <td>{{code}}</td>
      <td>{{temp}}</td>
      <td>{{windSpeed}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
</section>